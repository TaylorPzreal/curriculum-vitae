  "scripts": {
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "tslint": "tslint -c ./tslint.json './src/app/**/*.ts'",
    "dll": "rimraf dll && webpack --config config/webpack.dll.js --profile",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --colors --profile --hot",
    "start:aot": "webpack-dev-server --port=9000 --config config/webpack.aot.js",
    "startopen": "npm run start -- --open",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js --profile --colors --display-error-details",
    "build:aot": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack.aot.js",
    "test": "karma start",
    "e2e": "node_modules/protractor/bin/webdriver-manager update && node_modules/protractor/bin/protractor ./protractor.conf.js",
    "upgrade:angular": "npm update @angular/{animations,common,compiler,core,forms,http,platform-browser,platform-browser-dynamic,router,cli,compiler-cli,platform-server} @ngtools/webpack"
  },
  "pre-commit": [
    "precommit-msg",
    "tslint"
  ],
